generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MyClient {
  id            Int       @id @default(autoincrement())
  name          String
  slug          String    @unique
  is_project    String    @default("0")
  self_capture  String    @default("1")
  client_prefix String
  client_logo   String    @default("no-image.jpg")
  address       String?
  phone_number  String?
  city          String?
  created_at    DateTime  @default(now())
  updated_at    DateTime  @default(now())
  deleted_at    DateTime?
}
